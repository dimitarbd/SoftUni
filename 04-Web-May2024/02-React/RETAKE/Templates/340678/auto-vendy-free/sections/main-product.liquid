               </div>
                {%- if block.settings.make_collapsible_row -%}
                     </details>
                {%- endif -%}
                </div>

               {%- when 'timer' -%}
               {%- if product.metafields.custom.data_timer != blank -%}
                  {% assign launch_date = product.metafields.custom.data_timer.value %}
